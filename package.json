{
  "name": "base-express-project",
  "version": "1.0.0", 
  "main": "app.js",
  "engines": {
    "node": "14.17.0"
  },
  "scripts": {
    "start": "node app.js", 
    "dev": "nodemon app",
    "stop": "taskkill -f -im node.exe",
    "migrate": "npx knex migrate:latest --knexfile knexfile.js",
    "migrate:up": "npx knex migrate:up --knexfile knexfile.js",
    "migrate:down": "npx knex migrate:down --knexfile knexfile.js",
    "migrate:rollback": "npx knex migrate:rollback --knexfile knexfile.js",
    "migrate:rollback all": "npx knex migrate:rollback --all --knexfile knexfile.js",
    "migrate:list": "npx knex migrate:list --knexfile knexfile.js",
    "seed:run": "npx knex seed:run --knexfile knexfile.js"
  },
  "dependencies": {
    "ansi-regex": "^6.0.1",
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "faker": "5.5.3",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.0",
    "nodemon": "^2.0.20"
  }
}
